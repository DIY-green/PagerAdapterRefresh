<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diygreen.pageradapterrefresh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleDemoActivity" />
        <activity
            android:name=".PagerAdapterActivity"
            android:label="PagerAdapter" />
        <activity
            android:name=".FPagerAdapter1Activity"
            android:label="FPagerAdapter1" />
        <activity
            android:name=".FPagerAdapter2Activity"
            android:label="FPagerAdapter2"/>
        <activity
            android:name=".FSPagerAdapterActivity"
            android:label="FSPagerAdapter" />
    </application>

</manifest>
